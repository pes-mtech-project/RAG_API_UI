{
    "name": "finbert-rag-infrastructure",
    "version": "1.0.0",
    "description": "AWS CDK Infrastructure for FinBERT RAG ECS Deployment",
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "test": "jest",
        "cdk": "cdk",
        "deploy:dev": "cdk deploy FinBertRagDevStack --require-approval never",
        "deploy:prod": "cdk deploy FinBertRagProdStack --require-approval never",
        "destroy:dev": "cdk destroy FinBertRagDevStack --force",
        "destroy:prod": "cdk destroy FinBertRagProdStack --force",
        "diff": "cdk diff",
        "synth": "cdk synth"
    },
    "devDependencies": {
        "@types/jest": "^29.4.0",
        "@types/node": "20.1.7",
        "jest": "^29.5.0",
        "ts-jest": "^29.1.0",
        "aws-cdk": "2.100.0",
        "ts-node": "^10.9.1",
        "typescript": "~5.0.4"
    },
    "dependencies": {
        "aws-cdk-lib": "2.100.0",
        "constructs": "^10.0.0"
    }
}